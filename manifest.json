{

  "manifest_version": 2,
  "name": "justeat-enhancement-suite",
  "version": "1.0",

  "description": "Improved browsing experience for JustEat.",

  "browser_action": {
    "default_title": "Just-East Enhancement Suite",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://www.just-eat.co.uk/restaurants-*/menu"
  ]
}